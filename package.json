{
  "name": "wezeshanet-hotspot-js",
  "type": "module",
  "version": "0.0.11",
  "description": "Javascript library to display hotspot ui for wezeshanet",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./lib/web.js"
    }
  },
  "main": "lib/web.js",
  "types": "lib/web.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "release": "pnpm build --mode production && npx bumpp && npm publish"
  },
  "dependencies": {
    "@unocss/reset": "^0.58.2",
    "@vueuse/core": "^10.7.1",
    "lucide-vue-next": "^0.303.0",
    "radix-vue": "^1.2.6",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@tanstack/vue-table": "^8.11.2",
    "@types/node": "^20.10.6",
    "@unocss/preset-mini": "^0.58.2",
    "@vitejs/plugin-vue": "^4.5.2",
    "bumpp": "^9.2.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint": "npm:eslint-ts-patch@^8.56.0-0",
    "tailwind-merge": "^2.2.0",
    "typescript": "^5.2.2",
    "unocss": "^0.58.2",
    "vite": "^5.0.8",
    "vite-plugin-css-injected-by-js": "^3.3.1",
    "vite-plugin-dts": "^3.7.0",
    "vue-tsc": "^1.8.25"
  }
}
